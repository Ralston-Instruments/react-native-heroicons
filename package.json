{
  "name": "react-native-heroicons",
  "version": "1.5.0",
  "description": "React Native components for heroicons",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "sideEffects": false,
  "scripts": {
    "fetch": "rimraf heroicons/ && git clone https://github.com/tailwindlabs/heroicons/",
    "gen": "yarn fetch && ts-node --project bin/tsconfig.json --files bin/build.ts",
    "cleanup": "./cleanup.sh",
    "compile": "esbuild src/index.ts --bundle --external:@babel --external:react-native --format=esm --minify --outfile=dist/index.js && tsc --emitDeclarationOnly --outDir dist",
    "build": "yarn gen && yarn compile && yarn cleanup"
  },
  "keywords": [
    "react",
    "react native",
    "heroicons",
    "heroiconsui"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecklf/react-native-heroicons"
  },
  "bugs": {
    "url": "https://github.com/ecklf/react-native-heroicons/issues"
  },
  "author": "ecklf",
  "license": "MIT",
  "devDependencies": {
    "@svgr/core": "^5.4.0",
    "@svgr/plugin-jsx": "^5.4.0",
    "@svgr/plugin-prettier": "^5.4.0",
    "@svgr/plugin-svgo": "^5.4.0",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-native": "^0.64.6",
    "esbuild": "^0.12.3",
    "junk": "^3.1.0",
    "react": "^17.0.2",
    "react-native-svg": "^12.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-native-svg": ">=9"
  },
  "dependencies": {}
}
